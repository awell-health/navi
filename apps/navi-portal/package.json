{
  "name": "navi-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "export": "next export",
    "generate-tokens": "NODE_ENV=development tsx --env-file=.env.local scripts/generate-secure-tokens.ts",
    "generate-ci-env": "node scripts/generate-ci-env.js",
    "lhci": "lhci autorun",
    "lint": "next lint",
    "serve:lhci": "NODE_ENV=production pnpm run start",
    "start": "next start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:performance": "tsx scripts/test-performance.ts --all-themes",
    "theme:sync": "tsx --env-file=.env scripts/theme-sync.ts --sync-all",
    "theme:sync:dry-run": "tsx --env-file=.env scripts/theme-sync.ts --sync-all --dry-run",
    "theme:sync:single": "tsx --env-file=.env scripts/theme-sync.ts --org",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rm -rf .next dist"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/experimental-nextjs-app-support": "^0.12.2",
    "@awell-health/navi-react": "workspace:*",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-core": "^0.13.8",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@upstash/redis": "^1.35.0",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/kv": "^3.0.0",
    "chrome-launcher": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.11.0",
    "jiti": "^2.4.2",
    "jose": "^6.0.11",
    "lighthouse": "^12.6.1",
    "lucide-react": "^0.515.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@edge-runtime/vm": "^5.0.0",
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@lhci/cli": "^0.15.0",
    "@storybook/addon-a11y": "^9.0.10",
    "@storybook/addon-docs": "^9.0.10",
    "@storybook/addon-onboarding": "9.0.10",
    "@storybook/addon-vitest": "^9.0.10",
    "@storybook/nextjs-vite": "^9.0.10",
    "@storybook/react-vite": "^9.0.10",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/browser": "3.2.3",
    "@vitest/coverage-v8": "3.2.3",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-storybook": "^9.0.10",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.0",
    "storybook": "^9.0.10",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5",
    "vitest": "^3.2.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
